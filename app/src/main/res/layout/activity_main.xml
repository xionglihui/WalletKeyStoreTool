<?xml version="1.0" encoding="utf-8"?>


<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <EditText
            android:id="@+id/edt_main_key"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <EditText
            android:id="@+id/edt_main_pwd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="请输入密码生成KeyStore文件" />

        <Button
            android:id="@+id/btn_pwd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="请输入密码生成KeyStore文件" />

        <EditText
            android:id="@+id/edt_main_keySotre"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <EditText
            android:id="@+id/edt_main_keystore"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="请输入keystore文本" />

        <Button
            android:id="@+id/btn_keystore"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="请输入keystore文本生成私钥" />


        <EditText
            android:id="@+id/edt_main_private"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </LinearLayout>
</ScrollView>

